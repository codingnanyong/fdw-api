{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": ["http://localhost:{port}", "http://localhost:{port}"],
  "FDW": {
    "HQ": {
      "ConnectionString": "Server={ip};Port={port};Database={Database};Username={id};Password={Password}"
    },
    "IoT": {
      "ConnectionString": "Server={ip};Port={port};Database={Database};Username={id};Password={Password}"
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:7020"
      }
    }
  }
}
